server <- function(input, output) {
filtered_data <- reactive({
diamonds %>%
filter(cut == input$cut,
price >= input$price_range[1],
price <= input$price_range[2],
color == input$color)
})
output$table <- renderDataTable({
filtered_data() %>%
select(input$show_vars)
})
output$histogram <- renderPlot({
ggplot(filtered_data(), aes(x = carat)) +
geom_histogram(binwidth = 0.1, fill = "blue", color = "black") +
labs(title = "Carat Distribution")
})
output$scatterplot <- renderPlot({
ggplot(filtered_data(), aes(x = depth, y = table)) +
geom_point() +
labs(title = "Scatterplot of Depth vs. Table")
})
observeEvent(input$refresh, {
updateSelectInput(session, "cut", selected = input$cut)
updateSliderInput(session, "price_range", value = input$price_range)
updateRadioButtons(session, "color", selected = input$color)
updateCheckboxGroupInput(session, "show_vars", selected = input$show_vars)
})
}
